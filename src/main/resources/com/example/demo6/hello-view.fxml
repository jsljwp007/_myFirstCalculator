<VBox alignment="CENTER" prefHeight="337.0" prefWidth="442.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="48.0" prefWidth="203.0" />
   <ToolBar prefHeight="40.0" prefWidth="200.0">
     <items>
         <ToolBar>
            <items>
             <Button mnemonicParsing="false" text="Button" />
            </items>
         </ToolBar>
     </items>
   </ToolBar>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
